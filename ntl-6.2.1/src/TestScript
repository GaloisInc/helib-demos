
echo
echo "---------------------------------"
echo "making CanZassTest"
make CanZassTest
echo "running CanZassTest"
./CanZassTest < CanZassTestIn > XXX
sh RemoveProg CanZassTest
if diff -b XXX CanZassTestOut
then
echo "CanZassTest OK"
else
echo "bad CanZassTest"
fi

